{
  "tunnelName": "syn-os-secure-tunnel",
  "description": "Secure development tunnel for Syn_OS team collaboration",
  "security": {
    "encryption": "AES-256-GCM",
    "authentication": "mTLS",
    "authorization": "RBAC",
    "logging": "comprehensive"
  },
  "endpoints": [
    {
      "name": "frontend-dev",
      "port": 3000,
      "protocol": "https",
      "security": {
        "requireAuth": true,
        "allowedOrigins": ["*.github.dev", "localhost"],
        "rateLimit": {
          "requests": 100,
          "window": "1m"
        }
      }
    },
    {
      "name": "api-dev",
      "port": 8080,
      "protocol": "https",
      "security": {
        "requireAuth": true,
        "allowedOrigins": ["*.github.dev"],
        "rateLimit": {
          "requests": 50,
          "window": "1m"
        }
      }
    },
    {
      "name": "secure-admin",
      "port": 8443,
      "protocol": "https",
      "security": {
        "requireAuth": true,
        "requireMFA": true,
        "allowedUsers": ["@syn-os/core-team"],
        "rateLimit": {
          "requests": 20,
          "window": "1m"
        },
        "ipWhitelist": []
      }
    },
    {
      "name": "monitoring",
      "port": 9000,
      "protocol": "https",
      "security": {
        "requireAuth": true,
        "allowedUsers": ["@syn-os/security-team", "@syn-os/core-team"],
        "readOnly": true
      }
    }
  ],
  "monitoring": {
    "enabled": true,
    "metrics": {
      "connectionCount": true,
      "dataTransfer": true,
      "errorRate": true,
      "responseTime": true
    },
    "alerts": {
      "suspiciousActivity": {
        "enabled": true,
        "threshold": "high",
        "notification": ["security@syn-os.org"]
      },
      "rateLimitExceeded": {
        "enabled": true,
        "notification": ["devops@syn-os.org"]
      },
      "unauthorizedAccess": {
        "enabled": true,
        "severity": "critical",
        "notification": ["security@syn-os.org", "admin@syn-os.org"]
      }
    }
  },
  "audit": {
    "enabled": true,
    "logLevel": "detailed",
    "retention": "90d",
    "compliance": ["SOC2", "ISO27001"],
    "events": [
      "connection_established",
      "connection_terminated", 
      "authentication_success",
      "authentication_failure",
      "authorization_denied",
      "data_transfer",
      "configuration_change",
      "security_incident"
    ]
  },
  "backup": {
    "enabled": true,
    "encryption": true,
    "schedule": "daily",
    "retention": "30d",
    "verification": true
  }
}