# Wget configuration for SynOS build system
# Prevents wget-log files from cluttering the project root

# Output log file location
# Use /dev/null to disable logging, or specify a path
logfile = /dev/null

# Alternative: Log to build directory (uncomment to use)
# logfile = build/logs/wget.log

# Timeout settings
timeout = 30
tries = 3

# Don't be too verbose
verbose = off

# User agent
user_agent = SynOS-Build-System/2.2
